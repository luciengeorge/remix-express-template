{
  "name": "remix-express-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@conform-to/react": "*",
    "@conform-to/zod": "*",
    "@epic-web/remember": "*",
    "@epic-web/totp": "*",
    "@headlessui/react": "*",
    "@heroicons/react": "*",
    "@paralleldrive/cuid2": "*",
    "@prisma/client": "*",
    "@radix-ui/react-checkbox": "*",
    "@radix-ui/react-label": "*",
    "@react-email/components": "*",
    "@remix-run/express": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "bcryptjs": "*",
    "chalk": "*",
    "class-variance-authority": "*",
    "close-with-grace": "*",
    "clsx": "*",
    "compression": "*",
    "crypto-js": "*",
    "express": "*",
    "express-rate-limit": "*",
    "helmet": "*",
    "isbot": "*",
    "litefs-js": "*",
    "morgan": "*",
    "react": "*",
    "react-dom": "*",
    "remix-flat-routes": "*",
    "remix-sitemap": "*",
    "remix-utils": "*",
    "sonner": "*",
    "tailwind-merge": "*",
    "tiny-invariant": "*",
    "vite-env-only": "*",
    "zod": "*"
  },
  "devDependencies": {
    "@faker-js/faker": "*",
    "@remix-run/dev": "*",
    "@tailwindcss/forms": "*",
    "@tailwindcss/nesting": "*",
    "@tailwindcss/typography": "*",
    "@types/bcryptjs": "*",
    "@types/compression": "*",
    "@types/express": "*",
    "@types/morgan": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "autoprefixer": "*",
    "cross-env": "*",
    "cssnano": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "msw": "*",
    "postcss-import": "*",
    "prettier": "*",
    "prettier-plugin-organize-imports": "*",
    "prettier-plugin-tailwindcss": "*",
    "prisma": "*",
    "tailwindcss": "*",
    "tailwindcss-animate": "*",
    "tsconfig-paths": "*",
    "typescript": "*",
    "vite": "*",
    "vite-tsconfig-paths": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
  ]
}
